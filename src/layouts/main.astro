---
import BaseLayout from "./base.astro";
import Header from "../components/header.astro";
import Footer from "../components/footer.astro";


type Props = {
  title: string;
  description: string;
};

const { title, description } = Astro.props;
---

<BaseLayout title={title} description={description}>
  <Header />
  <main>
    <slot />
  </main>
  <Footer /> 

</BaseLayout>